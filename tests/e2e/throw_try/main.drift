import std.console.out

fn may_throw(flag: Int) returns Int {
  if flag == 0 {
    return 0
  } else {
    throw drift_error_new_dummy(flag, "payload", "")
  }
}

fn main() returns Int {
  try {
    may_throw(1)
  } catch err {
    out.writeln("err")
    return 1
  }
  out.writeln("ok")
  return 42
}
