exception MyError(a: String, b: String)

fn make_error() returns Error {
  throw MyError(a = "AA", b = "BBB")
}

fn main() returns Int {
  try {
    make_error()
  } catch MyError(e) {
    val view = e.args
    val la: Int = view[view.a()].unwrap().len()
    val lb: Int = view[view.b()].unwrap().len()
    return la + lb
  }
  return 0
}
