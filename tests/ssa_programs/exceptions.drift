import std.console.out

fn foo(x: Int) returns Int {
  return x
}

fn might_fail(x: Int) returns Int {
  return x
}

fn main() returns Int {
  val y: Int = try foo(1) else 42
  out.writeln("computed y")

  try {
    out.writeln("before")
    might_fail(1)
  } catch err {
    out.writeln("caught")
  }

  val rc: Int = 0
  return rc
}
