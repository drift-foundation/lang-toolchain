exception MyError(a: String, b: String)

fn make_error() returns Void {
  throw MyError(a = "A", b = "B")
}

fn main() returns Int {
  var sum: Int = 0
  try {
    make_error()
  } catch MyError(e) {
    val view = e.args
    val a_key = view.a()
    val b_key = view.b()
    // Access both keys; Option<String> results are assigned to ensure lowering hits __exc_args_get.
    val a_opt = view[a_key]
    val b_opt = view[b_key]
    // For now, just return 0; the SSA path is exercised by the lookups.
    sum = 0
  }
  return sum
}
