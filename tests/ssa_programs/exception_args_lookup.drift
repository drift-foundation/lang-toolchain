exception MyError(a: String, b: String)

fn make_error() returns Error {
  throw MyError(a = "A", b = "B")
}

fn main() returns Int {
  var sum: Int = 0
  try {
    make_error()
  } catch MyError(e) {
    val view = e.args
    val a_key = view.a()
    val b_key = view.b()
    val a_opt = view[a_key]
    val b_opt = view[b_key]
    sum = a_opt.unwrap().len() + b_opt.unwrap().len()
  }
  return sum
}
